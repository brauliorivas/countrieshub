{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./src/",
    "paths": {
      "@assets": ["assets/*"],
      "@components/*": ["components/*"],
      "@containers/*": ["containers/*"],
      "@context/*": ["context/*"],
      "@hooks/*": ["hooks/*"],
      "@pages/*": ["pages/*"],
      "@routes/*": ["routes/*"],
      "@styles/*": ["styles/*"], 
    }
  },
  "include": ["src", "index.d.ts"]
}